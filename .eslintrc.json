{
    "extends": [
      "eslint:recommended",
      "google",
      "prettier"
    ],
    "plugins": [
      "mocha"
    ],
    "parserOptions": {
      "ecmaVersion": 8
    },
    "rules": {
      "mocha/no-exclusive-tests": "error",
      "require-jsdoc": "off",
      "no-invalid-this": "off",
      "strict": ["error", "global"]
    },
    "env": {
      "mocha": true,
      "node": true,
      "es6": true
    }
  }
  